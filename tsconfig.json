{
	"ts-node": {
    "compilerOptions": {
      "module": "commonjs"
    }
  },
	"compilerOptions": {
			"target": "es6",
			"module": "commonjs",
			"outDir": "./dist/server",
			"esModuleInterop": true,
			"strict": true,
			"sourceMap": true,
			"rootDir": "src",
			"typeRoots": [
					"node_modules/@types"
			]
	},
	"include": ["src/**/*"],
	"exclude": [
			"dist",
			"node_modules"
	]
}
